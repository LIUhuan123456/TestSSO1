      "limit": 4
    },
    "clientNumber": {
      "type": "string",
      "required": false,
      "limit": 32
    },
    "disable": {
      "type": "boolean",
      "default": false
    },
    "hasAccount": {
      "type": "boolean",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
